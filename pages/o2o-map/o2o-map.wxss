page{
    background: #f4f4f4;
    font-family: Helvetica Neue,Tahoma,Arial,PingFangSC-Regular,Hiragino Sans GB,Microsoft Yahei,sans-serif;
    font-size: 16px;
}
.wrap{
    background: #fff;
}
.search-input{
    background: #1BAAD5;
    padding-bottom: 10px;
}
.search{
    width: 90%;
    margin: 0 auto;
    background: #ffffff;
    text-align: center;
}
.title{
    background: #f4f4f4;
    padding: 8px 0 8px 10px ;
}
.address-box{
    display: flex;
    padding: 8px 0;
}
.address{
    flex: 1;
    padding-left: 14px;
    color: #1c91fb;
    background: #ffffff;
}
.locationBtn{
    display: flex;
    align-items: center;
}
.Btn-icon{
    background: url("data:image/png;base64,") no-repeat;
    width: 16px;
    height: 16px;
    background-size: 100%;
}
.Btn-text{
    padding: 0 10px;
}
.nearby{

}
.nearbyAdress{
    background: #f4f4f4;
    padding: 8px 0 8px 10px ;
}
.SeleNearbyAdress{
    padding-left: 12px;
    font-size:14px;

}
.address-title{
    padding-left: 14px;
}
.address-item{
    padding: 10px 0;
    border-bottom: 1px solid #9e9e9e;
}