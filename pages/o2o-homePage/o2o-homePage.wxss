.page{
    font-family: 微软雅黑;
}
.main-bar{
    position: relative;
    background: #1BAAD5;

}
.GPS-bor{
    text-align: center;
    display: flex;
    align-items: center;
    font-size: 14px;
    color: #fff;
    padding-top: 10px;
    padding-left: 7%;
}
.search-bar{
    margin: 5px 0;
    overflow: hidden;
}
.search-inside{
    width:90%;
    margin: 0 auto;
    height: 28px;
    background: #fff;
    border-radius: 20px;
    display: flex;
    justify-content: center;
    align-items: center;
}

.search-inside>view{
    font-size: 14px;
    color: #b2aeae;
    margin-left: 5px;
}
.search-on-btn{
    width:90%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 0 auto;
    font-size: 14px;
    color: #fff;
    margin-bottom: 5px;
}
.To-btn{
    left: 5%;
    width: 90%;
    height: 90px;
    padding: 0 30px;
    box-sizing: border-box;
   display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 13px;
    text-align: center;
    background: #fff;
    border-radius: 20px;
    position: absolute;
    box-shadow: 0px 1px 10px #DBE8EB;
}
.To-btn-itme>view{
    margin-top: 5px;
    color: #929293;
}
.BJ-EFEFEF{
    margin-top: 50px;
    height: 60px;
    background: #EFEFEF;
}
.swiperImage{
    width: 100%;
    height: 100%;
}
.seller-bar{
margin-top:10px;
}
.seller-itme{
    padding: .5rem 0;
    border-bottom: 1px solid #f1f1f1;
}
.seller-itme-top{
    display: flex;
    padding: 0 15px;

}
.seller-info{
    flex:1;
    margin-left:.5rem;
    border-bottom: 1px solid #f1f1f1;
}
.seller-image{
    width: 4rem;
    height: 4rem;
    border:1px solid #f1f1f1;
}
.seller-volume-bar{
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 15px;
    margin: .4rem 0;
}
.price-bar,.price-info{
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-family: 微软雅黑;
}
.price-bar{
    font-size: 16px;
}
.seller-name{
    font-weight: 600;
    font-size: 18px;

}
.price-info{
    font-size: 12px;
}
.price-bar{
    margin-top: 10px;
}
.activity-info{
    box-sizing: border-box;
    padding-left: 5.5rem;
    margin: 0 auto;
}
.ZS-icon{
    background-color:rgb(35,149,255);
    color: #fff;
    font-size: 12px;
    padding: 2px;
    border-radius: 3px;
}
.PS-price{
    border-right: 1px solid #f1f1f1;
    padding: 0 5px;
}
.PS-price:last-child{
    border-right: none;
}
.PS-time{
    color:rgb(35,149,255);
    font-size: 12px;
}
.activity-info view{
    display: inline-block;
    font-size: 13px;
}
.YH-icon{
    width: 1rem;
}
.YH-icon{
    background: red;
    text-align: center;
    border-radius: 3px;
    color: #ffffff;
    margin-right:5px;
}
