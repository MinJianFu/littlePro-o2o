page,.page{
    width: 100%;
    height: 100%;
    background: #f1f1f1;
    font-family: 微软雅黑;
}
.queryval{
    height: 1.5rem;
    font-size: 12px;
    background: #fff;
    margin-bottom: .5rem;
    padding: .5rem;
}
.address-item{
    display: flex;
    align-items: center;
    font-size: 12px;
    background: #fff;
    padding: .3rem .5rem;
    border-bottom: 1px solid #f1f1f1;
}
.address-item>view{
    flex: 1;
}
.icon{
    width: 20px;
    height: 20px;
    margin-right: .5rem;
    display: block;
}
.address-XQ{
    margin-top: .3rem;
    color: #888888;
}

.PS-view{
    text-align: center;
    font-size: 13px;
    margin-top: 20px;
}
.PS-view>view:nth-child(1){
    font-size: 16px;
    margin-bottom: 10px;
}