page{height: 100%;background: #f1f1f1; font-family: 微软雅黑;}
.gooos-bar{
    display: flex;
    background: #ffffff;
    align-items: center;
    margin:.4rem 0;
    padding-left: .5rem;
    height: 3rem;
}
.gooos-barNew{
    display: flex;
    background: #ffffff;
    align-items: center;
    padding-left: .5rem;
    height: 2rem;
}
.gooos-bar .on{
    color: #c3c3c3;
}
.goos-titleNew{
    color: #c3c3c3;
    font-size: 12px;
}
.goos-title{
    font-size: 12px;
}
.gooos-name{
    background-size: 1rem;
    flex: 1;
    font-size: 12px;
    text-align: right;
}
.gooos-nameNew{
    background-size:1rem;
    flex:1;
    font-size:12px;
    text-align:left;
    text-indent:10px;
}
.goos-icon{
    width: 2rem;
    height: 1.5rem;
    text-align: center;

}
.gooos-bar>image{
    width:18px;
    height:18px;
    margin:0 .5rem 0;
}
.address-bar{
    background: #fff;
    margin-bottom: .4rem;
    padding:0 .5rem;
}
.address-bar>view{
    display: flex;
    align-items: center;
    height: 4rem;
}
.address-bar>view .address-text{
    flex: 1;
    color: #777777;
    font-size: 14px;
}
.address-textNew{
    flex: 1;
    color: #c3c3c3;
    font-size: 14px;
}
.address-bar>view .JT-icon{
    width: 20px;
    height: 20px;
}
.address-bar .address-icon{
    font-size: 11px;
    padding: 2px;
    border: 1px solid;
    border-radius: 50%;
    margin-right: .5rem;
    width: 13px;
    height: 13px;
    line-height: 11px;
    text-align: center;
}
.B1,.B2{
    background: #ffffff;
    margin-bottom: .4rem;
    font-size: 13px;
}
.peiStime-bar,.remarks-bar,.goos-pcier,.peiSpcier-bar,.voucher,.XF-bar{
    display: flex;
    height: 2rem;
    justify-content: space-between;
    align-items: center;
    padding:.5rem;
}
.peiStime-bar,.goos-pcier,.peiSpcier-bar,.voucher{
    border-bottom: 1px solid #f1f1f1;
}
.remarks-text,.goos-pcier-val{
    text-align: right;
}
.JT-icon{
    width: 20px;
    height: 20px;

    opacity: .5;
}
.voucher-text,.XF-val{
    flex: 1;
    text-align: right;
}
.goos-pcier-ps{
    margin-left: .3rem;
    color: #FA1A43;
}
.peiSpcier-bar-ps{
    margin-left: .3rem;
    color:#FA1A43 ;
    border:1px solid;
    padding: 2px;
    border-radius: 50%;
    width: 10px;
    height: 10px;
    display: inline-block;
    text-align: center;
    line-height: 9px;
}
.XF-bar-ps{
    margin-left: .3rem;
    color: #808080;
}
.foot{
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 2.5rem;
    display: flex;
    background: #fff;
    line-height: 2.5rem;
    font-size: 12px;
    font-family: 微软雅黑;
}
.foot view:nth-child(1){
    margin-left: .5rem;
}
.foot view:nth-child(2){
    flex: 1;
    text-align: right;
    margin-right: .3rem;
}
.TJ-btn{
    padding: 0 1rem;
    background: #FA1A43 ;
    color: #ffffff;
}
.foot txet{
    color: #FA1A43;
}
.time-sele-bar{
    display: flex;
    justify-content: space-between;
    padding: 0 .5rem;
    background: #fff;
    border-bottom: 1px solid #f1f1f1;
    font-size: 14px;
    line-height: 1.5rem;
}
.sele-bar{

    background: #ffffff;
    font-size: 12px;
    position: absolute;
    bottom: 0;
    width: 100%;
    transition: all .5s;
    transform: translateY(300px);
}
.sele-outer{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,.6);
    z-index: 2;
    display: none;
}
.seleon{
    transform: translateY(0px);
}
.sele-outer-on{
    display: block;
}