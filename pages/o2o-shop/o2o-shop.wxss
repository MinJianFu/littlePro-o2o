.Carmark{
    width: 100% ;
    height: 100%;
    position: absolute;
    top:-50px;
    left: 0;
    background: rgba(0,0,0,0.2);
}
.cartview{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    font-size: 14px;
    background: #fff;
    opacity: 0;
    z-index: -1;
    transition:all 0.5s ease;
    transform: translate(0,0);
    opacity: 0;
}
.CarAnimateIn{

    transform: translate(0,-47px);
    opacity: 1;
}

.hidden{
    display: none;
}
