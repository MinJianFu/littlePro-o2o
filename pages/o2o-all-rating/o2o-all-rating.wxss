.header-wrap{
    background: #fff;
    color: #242424;

}
.header-content{
    display: flex;
    display: -webkit-flex;
    position: relative;
    padding:10px;
    margin-left:5px;

}
.header-left{
    width: 80px;
}
.header-left-img{
    width: 80px;
}
.header-right{
    flex: 1;
}
.header-right-title {
    display:flex;
    font-size:14px;
    font-weight:700;
    margin-top:15px;
    margin-bottom:8px;
    line-height:12px;
    padding-left:6px;

}

.header-right-title-icon {
    width:17px;
    height:12px;
    border-radius:1.5px;
    background:#F7C709;
    padding:0 3px;
}
.header-right-title-content{
    margin-left: 5px;
}
.header-right-msg{
    font-size: 10px;
    padding-bottom: 15px;
    display: flex;
    font-family:微软雅黑;
    height:10px;
    line-height:10px;
}
.header-right-msg-B ,.header-right-msg-C{
    border-left: 1px solid #c2c2c2;
}
.header-right-msg-B ,.header-right-msg-C,.header-right-msg-A{
    padding:0 5px;

}
.header-right-info{
    font-size:10px ;
    Width:200px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding-left:5px;
    font-family:微软雅黑;
}
.header-right-icon{
    position: absolute;
    top:50%;
    right: 10px;
}
.header-right-icon-R{
    width: 12px;
    height:12px;
}


.header-right-star{
    font-size: 11px;
    display: flex;
    align-items: center;
    margin:0 0 5px 5px;

}

.header-bottom-info{
    padding-bottom:10px;
    width: 100%;
}
.hbi-distr, .hbi-info {
    display: flex;
    font-size: 12px;
    padding: 10px 2px;
    color: #9e9e9e;
    background: #f4f4f4;
    width: 90%;
    margin: 0 auto;
    line-height: 18px;
}
.hbi-distr{
    border-bottom: 1px solid #f1f1f1;
}

.hbi-distr-l,.hbi-info-l{
    padding:0 0 0 30px;
    flex:0 0 50px;

}
.halving-line{
    height: 10px;
    background: #eee;
}
.rating-content{
     padding: 0 10px;
 }
.rating-title {
    position:relative;
    font-size:12px;
    height:20px;
    border-bottom:1px solid #f1f1f1;
    line-height:20px;
    padding:10px 0;

}

 .rt-left{
     position: absolute;
     left: 10px;
 }
 .num-rating{
     position: absolute;
     right: 10px;
     display: flex;
     align-items: center;
 }
 .score-rating-icon{
     width: 12px;
     height: 12px;
 }
.see-rating {
    padding:10px 0;
    border-bottom:1px solid #f1f1f1;
    width:96%;
    margin-left:4%;
}

.rating-tab {
    display:flex;
    border-bottom:1px solid #f1f1f1;
    font-size:12px;
    padding:10px 0;
    width:96%;
    margin-left:4%;
}
.hrs-star{
    display: inline-block;
    margin:0 2px 0 0;
}
.rating-tab-box{
    width: 100%;
    margin-left:-1%;
}
.rating-num-a,.rating-num-b,.rating-num-c{
    color: #67727f;
    background: #ebf5ff;
    padding: 5px;
    border-radius: 5px;
    display: inline-block;
    margin: 4px;
}
.rating-num-a-num,.rating-num-b-num,.rating-num-c-num{
    display: inline-block;
}
.rating-active{
    color: #ffffff;
    background: #1c91fb;
}

.sr-title{
    display: flex;
    font-size: 14px;
    padding:15px 0 25px 0;
    color: #727272;
}

.ri-portrait{
    background: #a3d3fc;
    border-radius: 50%;
    width:30px;
    height: 30px;
    margin-right: 10px;
}
.rip-icon{
    width: 100%;
    height: 100%;
}
.rating-info{
    display: flex;
    font-size: 12px;
}
.ri-info{
    width: 100%;
}
.rii-top{
    display:flex;
    width: 100%;
    padding-bottom: 5px;
}
.ri-name{
    color: #5a5a5a;
    flex:0 0 80%;
}
.rii-center{
    padding-bottom: 10px;
    display: flex;
}
.rib-start{
    padding-right: 5px;
}
.rib-time{
    color: #a8a8a8;
}
.see-all{
    text-align: center;
    font-size: 13px;
    height:40px;
    line-height:40px;
    color: #9e9e9e;
    position: relative;
}
.see-all-icon{
    position: absolute;
    top:13px;
    width: 12px;
    height: 12px;
}
.header-right-title-icon{
    margin-right: 4px;
}
.rii-bottom{
    font-size: 14px;
    color: #313131;
}
.ri-time{
    text-align: right;
    color: #a8a8a8;
}
.srt-icon-box{
    padding-right: 5px;

}
.srt-icon{
    width: 18px;
    height: 18px;
    display: inline-block;

}
.activity-wrap{
    padding: 10px 0 0 0 ;
}
.activity-title{
    font-weight: 600;
    height: 26px;
    line-height: 26px;
    font-size: 14px;
    border-bottom:1px solid #f1f1f1;
    padding:0 0 5px 5px;
}
.activity-content {
    font-size:12px;
    display:flex;
    height:30px;
    padding:7px 5px;
    line-height:19px;
}

.ac-icon{
    width: 20px;
    height: 20px;
}
.xin-icon,.jian-icon,.piao-icon{
    width: 100%;
    height: 100%;
}
.shopInfo-wrap{
    font-size: 12px;
    padding: 10px 0 0 0 ;
}
.ac-icon{
    display: inline-block;
    padding-right: 5px;
}
.shopInfo-license{
   position: relative;
}
.shopInfo-title{
    font-weight: 600;
    height: 26px;
    line-height: 26px;
    font-size: 14px;
    border-bottom:1px solid #f1f1f1;
    padding:0 0 5px 5px;
}
.right-icon{
    position: absolute;
    right: 0px;
}
.shopInfo-address, .shopInfo-time, .shopInfo-license {
    height:40px;
    line-height:40px;
    border-bottom:1px solid #f1f1f1;
    width:95%;
    margin-left:5%;
}
.on{
    width: 12px;
    height: 12px;
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100%;
}
.half{
    width: 12px;
    height: 12px;
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100%;
}
.off{
    width: 12px;
    height: 12px;
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100%;
}
