page{
    background: #f4f4f4;
    font-family: Helvetica Neue,Tahoma,Arial,PingFangSC-Regular,Hiragino Sans GB,Microsoft Yahei,sans-serif;
    font-size: 16px;
}
.wrap{
    background: #ffffff;
}
.Contacts-box{
    display: flex;
}
.Contacts-from{
    width: 100%;
}
.Contacts-input{
    padding: 5px 0;
}
.Contacts-label{
    display: flex;
    padding: 10px 0;
}
.man{
    display: flex;
    align-items: center;
}
.woman{
    display: flex;
    align-items: center;
    margin-left: 15px;
}
.Contacts-label-icon{
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100%;
    width: 16px;
    height: 16px;
}

.Contacts-label-icon-h{
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100%;
    width: 16px;
    height: 16px;
}
.Contacts-number-box{
    display: flex;
    padding:5px 0;
}
.Contacts-label-text{
    margin-left: 5px;
}
.Contacts-number{
    flex:0 0 100px;
    display:flex;
    align-items:center;
    justify-content:center;
}
.sele-adress{
    display: flex;
}
.adress,.Contacts{
    flex:0 0 100px;
    display:flex;
    justify-content:center;
    padding-top: 5px;
}
.adressShow{
    width: 100%;
}
.adressText{
    padding: 10px 0;
}
.fliiadress{
    padding: 10px 0;
}
.Contacts-number-box,.Contacts-input,.Contacts-box,.sele-adress,.adressText{
    border-bottom: 1px solid #f4f4f4;
}
.btn {
    width:95%;
    margin:10px auto;
    color:#ffffff;
    background-color:#4cd964;
    border-radius:5px;
    display:block;
    height:35px;
    line-height:35px;
    text-align:center;
    font-size:12px;

}

