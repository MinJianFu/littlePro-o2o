page,.page{
    width: 100%;
    height: 100%;
    font-family: 微软雅黑;
    background: #f1f1f1;
}
.available{
    background: #fff;
    font-size:13px;
}
.item{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 12px;
    border-bottom: 1px solid #f1f1f1;
}
.add-icon{
    width: 15px;
    height: 15px;
}
.change-btn{
    width: 1.5rem;
    line-height: 1rem;
    text-align: center;
}
.title{
    line-height: 1rem;
    padding:.5rem;
    border-bottom: 1px solid #f1f1f1;
}
.user-info{
    margin-bottom: .3rem;
}
.not-available{
    background: #fff;
    font-size: 13px;
    color: #777777;
    margin-top: 1rem;
}
.add-btn{
    width:17rem;
    line-height:40px;
    margin:0 auto;
    background:#1c91fb;
    color:#fff;
    text-align:center;
    position:fixed;
    bottom:.5rem;
    left:0;
    right:0;
    font-size:13px;
    border-radius:.5rem;
    height:40px;
}